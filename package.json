{
  "name": "@getkoala/react",
  "version": "1.1.0",
  "description": "React hooks for integrating Koala.",
  "repository": "https://github.com/koala-live/react.git",
  "license": "MIT",
  "private": false,
  "author": "Matthew Shwery <matt@getkoala.com>",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "package.json"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:esm": "tsc --outDir dist/esm --module esnext",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {
    "react": "^16.9.0 || ^17.0.0"
  },
  "dependencies": {
    "@getkoala/browser": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react": "^17.0.38",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5"
  }
}
